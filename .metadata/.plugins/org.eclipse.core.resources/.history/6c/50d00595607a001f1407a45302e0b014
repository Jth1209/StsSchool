package spring;

import lombok.AllArgsConstructor;
import lombok.Getter;
import lombok.NoArgsConstructor;
import lombok.Setter;

@Getter
@Setter
@AllArgsConstructor
@NoArgsConstructor

public class Item {

	private int item_id;
	private String name;
	private int price;
	private int stock_quantity;
	
	
}
